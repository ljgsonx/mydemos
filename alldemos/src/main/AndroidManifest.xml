    <uses-permission android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS" />
    <permission android:name="com.example.administrator.alldemos.userprovider.AGREE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity1" />
        <activity android:name=".Activity2" />
        <activity android:name=".Activity3" />
        <activity android:name=".Activity4" />
        <activity android:name=".Activity5" />
        <activity android:name=".Activity6" />
        <activity android:name=".Activity7" />
        <activity android:name=".Activity8" />
        <activity android:name=".Activity9" />
        <activity android:name=".Activity10" />
        <activity android:name=".Activity11" />
        <activity android:name=".Activity12" />

        <provider
            android:exported="true"
            android:permission="com.example.administrator.alldemos.userprovider.AGREE"
            android:authorities="com.example.administrator.alldemos.userprovider"
            android:name=".UserProvider"/>
    </application>

</manifest>
